{
    "structure_version": 1.0,
    "protocol_version": 1.1,
    "configuration": {
        "name": "Arduino Uno with Rohm EVK-001, Rohm EVK-101 or A1 board",
        "doc": "Arduino Uno with Rohm EVK-001, Rohm EVK-101 or A1 board. Interrupts must be routed to pins 2 and/or 3",
        "bus1": {
            "connections": [{
                "connection": "i2c",
                "part_list": [{
                    "name": "KMX62",
                    "SAD": 15,
                    "int1_gpio": 3
                },
                {
                    "name": "KX122",
                    "SAD": 31,
                    "int1_gpio": 2
                },
                {
                    "name": "KXG03",
                    "SAD": 79,
                    "int1_gpio": 2
                },
                {
                    "name": "BM1383AGLV",
                    "SAD": 93,
                    "int1_gpio": 2
                },
                {
                    "name": "BM1422GMV",
                    "SAD": 14,
                    "int1_gpio": 2
                },
                {
                    "name": "RPR0521",
                    "SAD": 56,
                    "int1_gpio": 2
                },
                {
                    "name": "BH1745",
                    "SAD": 57,
                    "int1_gpio": 2
                }]
            }]
        },
        "bus2": {
            "connections": [{
                "connection": "USB",
                "packet_per_second": 400,
                "baud_rate": 1000000,
                "start_up_delay_ms": 2000,
                "com_port_name": ["Arduino Uno"]
            }]
        },
        "stream_config": {
            "directory": "stream_config/board_rokix_arduino_uno_evk"
        },
        "board_init": {
            "reg_write": []
        }
    }
}